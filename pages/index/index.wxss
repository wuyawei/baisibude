@import '../content/content.wxss';

.tabList{
  display: flex;
  justify-content: space-between;
  padding: 10rpx 28rpx;
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  box-sizing: border-box;
  z-index: 10;
  background-color: #eaeaea;
}
.tabList .active{
  color: #d43d3d;
}
.scrollnav{
  position: absolute;
  width: 100%;
  left: 0;
  top: 70rpx;
}